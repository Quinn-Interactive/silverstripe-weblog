{
	"name": "axllent/silverstripe-weblog",
	"description": "A blog module for SilverStripe",
	"type": "silverstripe-module",
	"homepage": "https://github.com/axllent/silverstripe-weblog",
	"keywords": ["silverstripe", "blog", "news"],
	"license": "The MIT License (MIT)",
	"authors": [
		{
			"name": "Ralph Slooten",
			"homepage": "http://www.axllent.org/"
		}
	],
	"support": {
		"issues": "https://github.com/axllent/silverstripe-weblog/issues"
	},
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/silverstripe/silverstripe-lumberjack"
        }
    ],
    "require": {
        "silverstripe/cms": "^4.0",
        "silverstripe/assets": "^1.0@dev",
        "silverstripe/asset-admin": "^1.0@dev",
        "silverstripe/lumberjack": "dev-master@dev"
	},
    "autoload": {
        "psr-4": {
            "Axllent\\Weblog\\": "src/"
        }
    },
    "suggest": {
        "axllent/silverstripe-weblog-categories": "optional: add categories to your blog posts"
    },
    "conflict": {
        "silverstripe/blog": "*"
    }
}
